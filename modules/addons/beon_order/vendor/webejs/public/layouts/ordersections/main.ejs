<div class="panel<% if (crosssaleparent != 0) { %> panel-cross-sales modal fade<% } %>" id="panel<%= index %>" data-type="product" data-index="<%= index %>" data-pid="<%= pid %>" <% if (crosssaleparent != 0) { %> tabindex="-1" role="dialog" aria-labelledby="crossSaleModalLabel" aria-hidden="true"<% } %>>
    <% if (crosssaleparent != 0) { %>
     <div class="modal-dialog modal-lg" role="document">
     <div class="modal-content">
     <% } %>
        <div class="panel-heading panel-heading-order">
            <div class="flex flex-row justify-space-between">
                <div class="header-left-panel">
                    <h3 class="title">Layanan yang kamu beli - <%= groupname %></h3>
                </div>
                <div class="header-right-panel">
                    <div class="flex">
                        <span class="domain-name"></span>
                        <span class="maximize-icon pointer hidden"><i class="fas fa-plus"></i></span>
                        <span class="minimize-icon pointer"><i class="fas fa-minus"></i></span>
                        <span class="remove-panel pointer"><i class="fas fa-trash-alt"></i></span>
                    </div>
                </div>
            </div>
        </div>
        <div class="panel-body panel-body-order">
            <div class="minimize-panel hidden">

            </div>

            <div class="maximize-panel">
                <div class="col-md-6">
                    <div class="section-pilihpaket">

                    </div>
                </div>
                <div class="col-md-6">
                    <div class="section-billingcycle">

                    </div>
                </div>
                <div class="col-md-12">
                    <div class="section-freedomain">

                    </div>
                </div>

                <div class="col-md-12 mb-10">
                    <div class="section-upsale">

                    </div>
                </div>

                <div class="col-md-12">
                    <div class="section-cross-sales">

                    </div>
                </div>

                <div class="col-md-12">
                    <div class="section-penawaran">

                    </div>
                </div>

                <div class="col-md-12">
                    <div class="section-configoptions">
                    </div>
                </div>

                <div class="col-md-12">
                    <div class="section-customfields">
                    </div>
                </div>

                <div class="col-md-12">
                    <div class="section-tambah-domain">

                    </div>
                </div>
                <div class="col-md-12">
                    <div class="section-domain-serupa"></div>
                </div>
                <div class="col-md-12">
                    <div class="section-area-footer"></div>
                </div>
            </div>
        </div>
    <% if (crosssaleparent != 0) { %>
        </div>
        </div>
    <% } %>
</div>