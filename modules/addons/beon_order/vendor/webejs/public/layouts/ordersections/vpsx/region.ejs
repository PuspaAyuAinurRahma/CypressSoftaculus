<h4 class="form-title">
    Pilih lokasi server
</h4>

<div class="row">
    <% configs.options.forEach(function(config) { %>
    <div class="col-md-6">
        <div class="region <%= config.id == selected.selectedvalue ? 'checked' : ''%>"
             data-region="<%= config.id %>" data-configid="<%= configs.id %>">

            <% if (config.name.toLowerCase().includes('id')) { %>
            <div class="img">
                <img src="/modules/addons/beon_order/templates/assets/images/flag-indonesia.png"
                     alt="">
            </div>

            <div class="text">
                <p>Indonesia</p>
                <h4>ID</h4>
            </div>
            <% } %>

            <% if (config.name.toLowerCase().includes('sg')) { %>
            <div class="img">
                <img src="/modules/addons/beon_order/templates/assets/images/flag-sg.png"
                     alt="">
            </div>

            <div class="text">
                <p>Singapore</p>
                <h4>SG</h4>
            </div>
            <% } %>
        </div>
    </div>
    <% }) %>
</div>