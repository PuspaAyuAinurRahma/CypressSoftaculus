<div class="modal fade" id="beoncustomorderchoosepayment" role="dialog" style="display: none">
    <div class="modal-dialog modal-sm">
        <!-- Modal content-->
        <div class="modal-content">
            <div class="modal-header payment-modal-header">
                <button type="button" class="close" data-dismiss="modal">Ã—</button>
                <h4 class="modal-title text-center text-bold">Pilih Metode Pembayaran</h4>
            </div>
            <div class="modal-body" style="padding: 0px">
                    <div class="panel-group panel-payment-groups" id="accordion-group-payment">
                        <div class="panel panel-default">
                            <div class="panel-heading payment-modal-subheading">
                                <div class="row">
                                    <div class="col-md-6"><h5 style="margin: 0px;"><strong>Pembayaran Instant</strong></h5></div>
                                    <div class="col-md-6 text-right;" style="text-align: right;">
                                        <a class="toggle-btn-hide" data-toggle="collapse" data-parent="#accordion" href="#panel-instan-payment"><small>sembunyikan <i class="fa fa-angle-up" aria-hidden="true"></i></small>
                                        </a>
                                    </div>
                                </div>
                            </div>
                            <div id="panel-instan-payment" class="panel-collapse panel-collapse-group collapse in">
                                <div class="panel-body">
                                    <div class="alert alert-success" style="margin-bottom: 0px;">
                                        <small>Pilihan metode pembayaran ini akan memastikan transaksi kamu otomatis tervalidasi saat itu juga, 24/7</small>
                                    </div>
                                    <div class="panel-group panel-paymentgateway-item" id="accordion" role="tablist" aria-multiselectable="true" style="margin-bottom: 0px;">

                                        <% payments.forEach(function(payment){ %>
                                        <div class="panel" style="margin-top: 0px; display: block;">
                                            <div class="panel-heading" role="tab" id="heading<%= payment.module %>" style="padding-top: 0px; padding-bottom: 0px;">
                                                <div class="panel-title payment-panel-title clearfix" style="font-size: 12px;">
                                                    <a role="button" data-trackingtitle="Pilih Metode Pembayaran <%= payment.module %>" data-trackingcategory="MainCTA" data-trackinglabel="Popup Payment Method" data-toggle="collapse" data-payment="<%= payment.module %>" data-parent="#accordion" href="#collapse<%= payment.module %>" aria-expanded="false" class="collapsed choose-payment-item-btn" aria-controls="collapse<%= payment.module %>">
                                                        <img src="/templates/orderforms/sobat_jagoan/images/<%= payment.module %>.png" class="bank-img" alt="">
                                                        <%= payment.displayname %>
                                                        <i class="more-less fa fa-angle-right pull-right" style="margin-top: 15px;font-size: 18px;"></i>
                                                    </a>
                                                </div>
                                            </div>
                                            <div id="collapse<%= payment.module %>" class="panel-collapse collapse panel-payment-tutorial" role="tabpanel" aria-labelledby="heading<%= payment.module %>" aria-expanded="false">
                                                <div class="panel-body">
                                                    <div class="row top-0">
                                                        <div class="col-md-12 text-center">
                                                            <label class="checkbox">
                                                                <input data-payment="<%= payment.module %>" type="checkbox" name="accepttos" class="accepttos" id="accepttos<%= payment.module %>">
                                                                Saya telah membaca dan setuju dengan <a href="https://www.jagoanhosting.com/aturan-layanan/#tos" target="_blank">Syarat & Ketentuan Layanan</a>
                                                            </label>
                                                        </div>
                                                        <div class="col-md-12">
                                                            <button data-payment="<%= payment.module %>" class="btn btn-pink btn-block btn-select-payment" id="btn-select-payment-<%= payment.module %>" disabled="true"> <i class="fa fa-spinner fa-spin hidden"></i> CHECKOUT SEKARANG</button>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <% }) %>

                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
            </div>
        </div>
    </div>
</div>